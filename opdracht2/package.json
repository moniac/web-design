{
	"name": "browserify-babel-async-await-demo",
	"description": "",
	"private": true,
	"license": "MIT",
	"scripts": {
		"dev": "yarn run kek && yarn run watch",
		"watch": "watchify ./src/index.js -t babelify -o ./dist/bundle.js -v",
		"clean": "rimraf ./dist",
		"build": "yarn run build:js ",
		"build:js":
			"yarn run kek && browserify ./src/index.js -t babelify | uglifyjs -cm > ./dist/bundle.js && yarn run build:css",
		"start": "yarn run watch",
		"build:css":
			"postcss ./src/styles/input.css -o ./dist/styles/output.css --verbose",
		"ok": "cpx './src/index.html' 'dist'",
		"kek": "htmlprocessor ./src/index.html -o './dist/'"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.6.1",
		"babelify": "^8.0.0",
		"browserify": "^16.1.1",
		"cpx": "^1.5.0",
		"cssnano": "^3.10.0",
		"htmlprocessor": "^0.2.6",
		"npm-run-all": "^4.1.2",
		"postcss-cli": "^5.0.0",
		"postcss-cssnext": "^3.1.0",
		"postcss-custom-properties": "^7.0.0",
		"postcss-import": "^11.1.0",
		"postcss-nesting": "^4.2.1",
		"postcss-uncss": "^0.16.1",
		"rimraf": "^2.6.2",
		"uncss": "^0.16.2",
		"watchify": "^3.11.0"
	},
	"dependencies": {}
}
