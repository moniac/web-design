{
  "name": "web-design",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:moniac/web-design.git",
  "author": "Mohammed Mulazada <thisismoniac@gmail.com>",
  "license": "MIT",
  "scripts": {
    "watch": "watchify ./app.js -o ./bundle.js -v",
    "build": "browserify ./app.js | uglifyjs -cm > ./bundle.js",
    "start": "npm run watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^16.1.1",
    "uglify-js": "^3.3.20",
    "watchify": "^3.11.0",
    "whatwg-fetch": "^2.0.4"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
  }
}
